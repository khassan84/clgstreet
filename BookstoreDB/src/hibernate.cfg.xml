<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>       
  <session-factory>
    <property name="dialect">org.hibernate.dialect.PostgreSQLDialect</property>
    <property name="show_sql">true</property> 
   <!--  <property name="hibernate.jdbc.batch_size">50</property> -->
   <property name="hibernate.hbm2ddl.auto">validate</property>
    
	<mapping resource="com/bookstoredb/bean/users.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/userpassword.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/questionpaper.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/event_a.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/event_i.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/event_t.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/files.hbm.xml"/> 
	<mapping resource="com/bookstoredb/bean/rack.hbm.xml"/>
	<mapping resource="com/bookstoredb/bean/event_i__event_a.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
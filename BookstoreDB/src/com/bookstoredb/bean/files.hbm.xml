<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bookstoredb.bean">
    <class name="FilesDO" table="files">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="name" column="name"/>
        <property name="aggregatedBy"  column="aggregated_by" />
        <property name="createDate"  type="date" column="create_date" />
		<property name="updateDate" type="date" column="update_date" />
		<property name="content" column="content" />
		
    </class> 
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bookstoredb.bean">
    <class name="EventIDO" table="event_i">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="name" column="name"/>
        <property name="idEventA"  column="id_event_a" />
        <property name="processRequire"  column="process_require" />
		<property name="processDate" type="date" column="process_date" />
		<property name="createDate" type="date"  column="create_date" />
		<property name="idQuestionPaper"  column="id_question_paper" />
		<!-- <set name="eventA" table="event_a" inverse="false" lazy="true" fetch="select" cascade="all">
			<key><column name="id_event_i" not-null="true"></column> </key>
			<many-to-many entity-name="com.bookstoredb.bean.EventADO">
			<column name="id_event_a" not-null="true"></column>
			</many-to-many>
		</set> -->
    </class> 
    <query name="findNextEventIGroup">
    <![CDATA[ from EventIDO where processRequire = true ]]>
    
    </query>
    
    <query name="get_event_i_by_ids">
    <![CDATA[  from EventIDO  where id in (:ids )]]>
    
    </query>
    
    
    
</hibernate-mapping>